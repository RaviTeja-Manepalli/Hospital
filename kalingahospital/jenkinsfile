node {
     
  stage('Git-Checkout') {
   git 'https://github.com/Shyamjith06/Hospital.git'
  }
    
 def project_path="kalingahospital"
 
 dir(project_path) {
    
  stage('Maven-Clean') {
   sh label: '', script: 'mvn clean'
  }
    
 stage('Maven-Compile') {
   sh label: '', script: 'mvn compile'
 }
   stage('Maven-Package') {
   sh label: '', script: 'mvn package'
  } 
}
}

